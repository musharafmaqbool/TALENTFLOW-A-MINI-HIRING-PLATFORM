{
  "rewrites": [
    { "source": "/mockServiceWorker.js", "destination": "/mockServiceWorker.js" },
    { "source": "/(.*)", "destination": "/" }
  ],
  "headers": [
    {
      "source": "/mockServiceWorker.js",
      "headers": [
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}

